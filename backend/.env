# Конфигурация для разработки

# SQLite база данных в корне проекта
DATABASE_URL=sqlite:///./database.db

# Секретный ключ для JWT (только для разработки!)
SECRET_KEY=new-secret-key-reset-by-user-request

# Токен живёт 60 минут
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Режим отладки включен
DEBUG=true

# CORS для локальной разработки
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://127.0.0.1:5500
